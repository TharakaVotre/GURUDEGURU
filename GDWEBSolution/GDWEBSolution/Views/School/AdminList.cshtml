@model IEnumerable<GDWEBSolution.Models.Schools.SchoolAdminModel>
<div class="row">
    <div class="col-md-12">
        @if (Model.Count() == 0)
        {
            <div class="col-md-12">
                <div class="well">
                    No Admin is set to display
                </div>
            </div>
        }
        else
        {
            foreach (var item in Model)
            {

                @*<div class="row">*@
                <div class="col-md-6">
                    <div class="well">
                        <i class="glyphicon glyphicon-bell"  aria-hidden="true"></i>
                        @Html.DisplayFor(modelItem => item.AdminName)
                        <button type="button" class="close" onclick='DeleteSchoolAdmin("@item.SchoolId","@(item.AdminUserId)");'>&times;</button>
                    </div>
                </div>

                @*</div>*@
            }
        }
    </div>
</div>
<script>

    function DeleteSchoolAdmin(SchoolId, AdminUserId) {
        debugger
        var Url = "/School/DeleteAdmin?SchoolId=" + SchoolId + "&AdminUserId=" + AdminUserId;
        $("#DeleteModelBody").load(Url, function () {
            $("#deleteModel").modal("show");
        })
    }
</script>